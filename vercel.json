{
  "version": 2,
  "name": "large-file-upload-system",
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "server.js"
    },
    {
      "src": "/health",
      "dest": "server.js"
    },
    {
      "src": "/(.*)",
      "dest": "index.html"
    }
  ],
  "regions": ["iad1"],
  "functions": {
    "server.js": {
      "maxDuration": 300,
      "memory": 3008
    }
  },
  "env": {
    "NODE_ENV": "production"
  }
}